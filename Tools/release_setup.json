{
  "JSON_SCHEMA_VERSION": "1.1",
  "PACKAGE_NAME": "KAS",
  "SHELL_COMPILE_BINARY_SCRIPT" : "make_binary.cmd",
  "RELEASE_NAME_FREE_FORMAT" : "{PACKAGE_NAME}_v{0}.{1}",
  "STRUCTURE" : {
    "/": [
      "/Deps/*",
      "-README.*"
    ],
    "" : [
      "/LICENSE.md",
      "/Lang",
      "/Models",
      "/Parts",
      "/Patches",
      "/Sounds",
      "/Textures",
      "/settings.cfg"
    ],
    "Plugins" : [
      "{COMPILED_BINARY}",
      "bin/Release/{PACKAGE_NAME}.xml",
      "{MINIAVC_VERSION_FILE}",
      "/Binaries/*",
      "-MiniAVC-V2.version"
    ]
  }
}
